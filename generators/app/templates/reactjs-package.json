    "gulp-replace": "^0.5.4",
    "gulp-rev": "^7.1.2",
    "gulp-rev-replace": "^0.4.3",
    "gulp-sass": "^3.1.0",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-srcset": "^1.1.0",
    "gulp-stylelint": "^3.8.0",
    "gulp-teleport": "^2.2.1",
    "gulp-util": "^3.0.8",
    "htmllint-config-trigen": "^1.1.0",
    "http2": "^3.3.6",
    "immutability-helper": "^2.1.1",
    "react-hot-loader": "next",
    "sass-modules-importer": "^1.2.1",
    "stylelint-config-trigen": "^1.6.1",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-manifest-plugin": "^1.3.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "useBuiltIns": true,
          "targets": {
            "browsers": [
              "> 1%",
              "last 2 versions",
              "iOS > 7",
              "Android > 4.4",
              "not OperaMini all"
            ],
            "node": "current"
          }
        }
      ],
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
