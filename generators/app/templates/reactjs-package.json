    "babel-preset-stage-0": "^6.22.0",
    "browser-sync": "^2.18.13",
    "chunk-manifest-webpack-plugin": "^1.1.2",
    "del": "^2.2.2",
    "eslint": "^3.19.0",
    "eslint-config-trigen": "^1.2.0",
