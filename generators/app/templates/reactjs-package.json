    "gulp-autoprefixer": "^3.1.1",
    "gulp-cache": "git+https://github.com/TrigenSoftware/gulp-cache.git#one-to-many",
    "gulp-cli": "^1.2.2",
    "gulp-cssnano": "^2.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-favicons": "^2.2.7",
    "gulp-html-linter": "^1.0.0",
    "gulp-htmlmin": "^3.0.0",
    "gulp-json-editor": "^2.2.1",
    "gulp-merge-json": "^1.0.0",
