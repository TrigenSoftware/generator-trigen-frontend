{
  "name": "generator-trigen-frontend",
  "version": "2.0.0-alpha",
  "description": "Yeoman generator for frontend development.",
  "author": "dangreen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrigenSoftware/generator-trigen-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/TrigenSoftware/generator-trigen-frontend/issues"
  },
  "main": "generators/app/index.js",
  "engines": {
    "node": ">=6.4.0"
  },
  "scripts": {
    "checkDeps": "npm-check -s; exit 0",
    "lint": "eslint --cache 'src/**/*.js' 'templates/**/*.js'",
    "test": "npm run checkDeps && npm run lint",
    "copyTemplates": "rm -rf generators/app/templates; mkdir -p generators/app; cp -R templates generators/app/templates",
    "build": "rollup -c && npm run copyTemplates",
    "watch": "rollup -c -w",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [
    "trigen",
    "frontend",
    "html",
    "scss",
    "js",
    "react",
    "yeoman-generator"
  ],
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.49",
    "chalk": "^2.4.1",
    "hasbin": "^1.2.3",
    "yeoman-generator": "^3.0.0",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.52",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-stage-0": "^7.0.0-beta.49",
    "eslint": "^5.1.0",
    "eslint-config-trigen": "^3.2.0",
    "husky": "^1.0.0-rc.10",
    "rollup": "^0.63.2",
    "rollup-plugin-babel": "^4.0.0-beta.5",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.3.0"
  },
  "files": [
    "generators"
  ]
}
